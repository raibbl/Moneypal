{
  "expo": {
    "name": "MoneyPal",
    "slug": "moneypal",
    "scheme": "moneypal",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "money.pal"
    },
    "android": {
      "package": "money.pal"
    },
    "web": {},
    "plugins": [
      "expo-font"
    ]
  }
}
